<%- include("partials/_header") %>

<div class="container mt-5">
  <div class="row">
    <div class="col-md-5 offset-md-3 card bg-dark">
      <div class="card-body">
        <form action="/EditarUsuario/<%= data.urs_id %>" method="POST">
          <input
            type="text"
            name="nombre"
            value="<%= data.urs_nombre %>"
            placeholder="Name"
            class="form-control mb-3"
            autofocus
          />
          <input
            type="text"
            name="email"
            value="<%= data.urs_email%>"
            placeholder="Address"
            class="form-control mb-3"
          />
          <input
            type="text"
            name="telefono"
            value="<%= data.urs_telefono%>"
            placeholder="Phone"
            class="form-control mb-3"
          />

          <button type="submit" class="btn btn-primary">Salvar Cambios</button>

        </form>
      </div>
    </div>
  </div>
</div>

<%- include("partials/_footer") %>
